# 9badc5dc6e69fa869bd0e611c4d72fa1c76a01362297aa4b624849615e287e1d Elan 1.6.5 guest default_profile valid

main
  let scale be [262, 294, 330, 349, 392, 440, 494, 523]
  let scaleL be scale.length()
  let quaver be 250
  let volume be 1.5
  for i from 0 to scaleL - 1 step 1
    call tone(quaver, scale[i], volume)
  end for
  call pause(1000)
  for i from 0 to scaleL - 1 step 1
    call tone(quaver, scale[scaleL - 1 - i], volume)
  end for
end main
