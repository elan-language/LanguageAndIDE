# 95f495ab3b91556290fe6d0473741f83215a8d5371d66ed0fd54000adde7791b Elan 1.8.0 guest default_profile valid

main
  let scale be [262, 294, 330, 349, 392, 440, 494, 523]
  let scaleL be scale.length()
  let quaver be 250
  let volume be 1.5
  for i from 0 to scaleL - 1 step 1
    call tone(quaver, scale[i], volume)
  end for
  call pause(1000)
  for i from 0 to scaleL - 1 step 1
    call tone(quaver, scale[scaleL - 1 - i], volume)
  end for
end main
