# 88a79c1512c1276a0047b6e619dd0b754d1fa10b9b9c5178ceb940491de17ce1 Elan 1.7.0 guest default_profile valid

main
  let scale be [262, 294, 330, 349, 392, 440, 494, 523]
  let scaleL be scale.length()
  let quaver be 250
  let volume be 1.5
  for i from 0 to scaleL - 1 step 1
    call tone(quaver, scale[i], volume)
  end for
  call pause(1000)
  for i from 0 to scaleL - 1 step 1
    call tone(quaver, scale[scaleL - 1 - i], volume)
  end for
end main
