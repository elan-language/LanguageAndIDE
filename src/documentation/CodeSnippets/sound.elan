# 156a54d516a07e8d98e6edc4da92495de4b861b1dad6535b7fa855b2396b4f70 Elan 1.9.0 guest default_profile valid

main
  let scale be [262, 294, 330, 349, 392, 440, 494, 523]
  let scaleL be scale.length()
  let quaver be 250
  let volume be 1.5
  for i from 0 to scaleL - 1 step 1
    call tone(quaver, scale[i], volume)
  end for
  call pause(1000)
  for i from 0 to scaleL - 1 step 1
    call tone(quaver, scale[scaleL - 1 - i], volume)
  end for
end main
