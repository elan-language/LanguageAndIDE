# c567d3837a177c7ee0709a1d7b4bb6f31341b1231f9cc27157d583cfb6f8ea7d Elan 2.0.0-alpha guest default_profile valid

function liveNeighbours(cells as List<of Boolean>, c as Int) returns Int
  variable neighbours set to neighbourCells(c)
  variable live set to neighbours.filter(lambda i as Int => cells[i])
  return live.length()
end function

function neighbourCells(c as Int) returns List<of Int>
  return [0]
end function
