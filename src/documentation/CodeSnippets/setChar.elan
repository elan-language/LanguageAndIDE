# d80d7f574e85a0020a6f18c801499610ba05cf9c81fe938c342a5c333f89d878 Elan 1.6.3 guest default_profile valid

main
  let s be "dependant"
  print setChar(s, 6, "e")
end main

function setChar(s as String, n as Int, c as String) returns String
  return s[..n] + c + s[n + 1..]
end function

test 
  assert setChar("ABCDE", 0, "_") is "_BCDE"
  assert setChar("ABCDE", 4, "_") is "ABCD_"
end test
