# 29390f2ab97571d4996d7844b128ade93ff29830668527b34cd814ac24f69e7b Elan 1.6.4 guest default_profile valid

main
  let s be "dependant"
  print setChar(s, 6, "e")
end main

function setChar(s as String, n as Int, c as String) returns String
  return s[..n] + c + s[n + 1..]
end function

test 
  assert setChar("ABCDE", 0, "_") is "_BCDE"
  assert setChar("ABCDE", 4, "_") is "ABCD_"
end test
