# 8f490830a5d72d2153bd6fe7188b8eb37d43b685ab8eda0e2138f258efcd2521 Elan 1.6.2 guest default_profile valid

main
  let s be "dependant"
  print setChar(s, 6, "e")
end main

function setChar(s as String, n as Int, c as String) returns String
  return s[..n] + c + s[n + 1..]
end function

test 
  assert setChar("ABCDE", 0, "_") is "_BCDE"
  assert setChar("ABCDE", 4, "_") is "ABCD_"
end test
