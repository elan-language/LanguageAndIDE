main
  let names be ["Abel", "Boole", "Cantor", "Descartes"]
  let file be createFileForWriting("Maths.txt")
  variable i set to 0
  repeat
    call file.writeLine(names[i])
    set i to i + 1
  end repeat when i is names.length()
  call file.saveAndClose()
end main
