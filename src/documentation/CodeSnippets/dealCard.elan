function dealCard(random as Float) returns Card
  variable number set to (random*52).floor()
  variable rank set to rankValue.keys()[(number/4).floor()]
  variable suit set to number mod 4
  return new Card() with rank set to rank, suit set to suit
end function

class Card
  property rank as Int

  property suit as Int

end class

constant rankValue set to {0:0}
