main
  let names be ["Tom", "Dick", "Harriet"]
  call print(names[0])
  each n in names
    call print(n)
  end each
  call print(reverse("Harriet"))
end main

function reverse(s as String) returns String
  variable sReturn set to empty String
  each ch in s
    set sReturn to ch + sReturn
  end each
  return sReturn
end function
