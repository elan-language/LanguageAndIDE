main
  variable names set to ["Tom", "Dick", "Harriet"]
  print names[0]
  each n in names
    print n
  end each
  print reverse("Harriet")
end main

function reverse(s as String) returns String
  variable sReturn set to empty String
  each ch in s
    set sReturn to ch + sReturn
  end each
  return sReturn
end function
